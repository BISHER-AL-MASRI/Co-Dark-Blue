{
	"$schema": "vscode://schemas/color-theme",
	"name": "Co-Dark Blue",
	"semanticHighlighting": true,
	"tokenColors": [
	  {
		"name": "String",
		"scope": "string",
		"settings": {
		  "foreground": "#3d19ca"
		}
	  },
	  {
		"name": "String Escape",
		"scope": "constant.character.escape, text.html constant.character.entity.named, punctuation.definition.entity.html",
		"settings": {
		  "foreground": "#2a12a7"
		}
	  },
	  {
		"name": "Boolean",
		"scope": "constant.language.boolean",
		"settings": {
		  "foreground": "#6a2fff"
		}
	  },
	  {
		"name": "Number",
		"scope": "constant.numeric",
		"settings": {
		  "foreground": "#0015ff"
		} 
	  },
	  {
		"name": "Identifier",
		"scope": "variable, support.variable, support.class, support.constant, meta.definition.variable, entity.name.function",
		"settings": {
		  "foreground": "#886aff"
		}
	  },
	  {
		"name": "Constant",
		"scope": "variable.other.constant",
		"settings": {
		  "foreground": "#6551ff"
		}
	  },
	  {
		"name": "Keyword",
		"scope": "keyword, modifier, variable.language.this, support.type.object, constant.language",
		"settings": {
		  "foreground": "#3533b9"
		}
	  },
	  {
		"name": "Keyword",
		"scope": "keyword.control.flow.js",
		"settings": {
		  "foreground": "#5d00ff"
		}
	  },
	  {
		"name": "Function call",
		"scope": "entity.name.function, support.function",
		"settings": {
		  "foreground": "#2400c6"
		}
	  },
	  {
		"name": "Storage",
		"scope": "storage.type, storage.modifier, keyword.operator.expression",
		"settings": {
		  "foreground": "#592fff"
		}
	  },
	  {
		"name": "Type",
		"scope": "support.type",
		"settings": {
		  "foreground": "#5d00ff"
		}
	  },
	  {
		"name": "Type",
		"scope": "entity.name.type, entity.other.inherited-class",
		"settings": {
		  "foreground": "#5200c5"
		}
	  },
	  {
		"name": "Class variable",
		"scope": "variable.object.property, meta.field.declaration entity.name.function",
		"settings": {
		  "foreground": "#3733b9"
		}
	  },
	  {
		"name": "Class method",
		"scope": "meta.definition.method entity.name.function",
		"settings": {
		  "foreground": "#3733b9"
		}
	  },
	  {
		"name": "Function definition",
		"scope": "meta.function, entity.name.function",
		"settings": {
		  "foreground": "#3733b9"
		}
	  },
	  {
		"name": "Template expression",
		"scope": "template.expression.begin, template.expression.end, punctuation.definition.template-expression.begin, punctuation.definition.template-expression.end",
		"settings": {
		  "foreground": "#4013c6"
		}
	  },
	  {
		"name": "Reset embedded/template expression colors",
		"scope": "meta.embedded, source.groovy.embedded, meta.template.expression",
		"settings": {
		  "foreground": "#FFFFFF"
		}
	  },
	  {
		"name": "YAML key",
		"scope": "entity.name.tag.yaml",
		"settings": {
		  "foreground": "#2e0a90"
		}
	  },
	  {
		"name": "Comment",
		"scope": "comment",
		"settings": {
			"foreground": "#3c0c9b"
		}
	},
	  {
		"name": "JSON key",
		"scope": "meta.object-literal.key, meta.object-literal.key string, support.type.property-name.json",
		"settings": {
		  "foreground": "#7b5eee"
		}
	  },
	  {
		"name": "JSON constant",
		"scope": "constant.language.json",
		"settings": {
		  "foreground": "#552fff"
		}
	  },
	  {
		"name": "CSS class",
		"scope": "entity.other.attribute-name.class",
		"settings": {
		  "foreground": "#552fff"
		}
	  },
	  {
		"name": "CSS ID",
		"scope": "entity.other.attribute-name.id",
		"settings": {
		  "foreground": "#3114a7"
		}
	  },
	  {
		"name": "CSS tag",
		"scope": "source.css entity.name.tag",
		"settings": {
		  "foreground": "#685eee"
		}
	  },
	  {
		"name": "HTML tag outer",
		"scope": "meta.tag, punctuation.definition.tag",
		"settings": {
		  "foreground": "#39119d"
		}
	  },
	  {
		"name": "HTML tag inner",
		"scope": "entity.name.tag",
		"settings": {
		  "foreground": "#4e26d2"
		}
	  },
	  {
		"name": "HTML tag attribute",
		"scope": "entity.other.attribute-name",
		"settings": {
		  "foreground": "#6600ff"
		}
	  },
	  {
		"name": "Markdown heading",
		"scope": "markup.heading",
		"settings": {
		  "foreground": "#5f2fff"
		}
	  },
	  {
		"name": "Markdown link text",
		"scope": "text.html.markdown meta.link.inline, meta.link.reference",
		"settings": {
		  "foreground": "#6b46ff"
		}
	  },
	  {
		"name": "Markdown list item",
		"scope": "text.html.markdown beginning.punctuation.definition.list",
		"settings": {
		  "foreground": "#712fff"
		}
	  },
	  {
		"name": "Markdown code block",
		"scope": "markup.fenced_code.block.markdown punctuation.definition.markdown",
		"settings": {
		  "foreground": "#3e14a7"
		}
	  },
	  {
		"name": "Markdown inline code",
		"scope": "markup.inline.raw.string.markdown",
		"settings": {
		  "foreground": "#3e14a7"
		}
	  },	
	  {
		"name": "Markdown Punctuation",
		"scope": [
		  "punctuation.definition.heading.markdown",
		  "punctuation.definition.string.begin.markdown",
		  "punctuation.definition.string.end.markdown",
		  "punctuation.definition.quote.begin.markdown",
		  "punctuation.definition.list.begin.markdown",
		  "markup.fenced_code.block.markdown",
		  "meta.separator.markdown"
		],
		"settings": {
		  "foreground": "#4035a0"
		}
	  },
	  
	  {
		"name": "INI property name",
		"scope": "keyword.other.definition.ini",
		"settings": {
		  "foreground": "#6246ff"
		}
	  },
	  {
		"name": "INI section title",
		"scope": "entity.name.section.group-title.ini",
		"settings": {
		  "foreground": "#552fff"
		}
	  },
	  {
		"name": "C# class method",
		"scope": "source.cs meta.method.identifier entity.name.function",
		"settings": {
		  "foreground": "#3733b9"
		}
	  },
	  {
		"name": "C# function call",
		"scope": "source.cs meta.method-call meta.method, source.cs entity.name.function",
		"settings": {
		  "foreground": "#4400ff"
		}
	  },
	  {
		"name": "C# type",
		"scope": "source.cs storage.type",
		"settings": {
		  "foreground": "#4533b9"
		}
	  },
	  {
		"name": "C# return type",
		"scope": "source.cs meta.method.return-type",
		"settings": {
		  "foreground": "#3733b9"
		}
	  },
	  {
		"name": "C# preprocessor",
		"scope": "source.cs meta.preprocessor",
		"settings": {
		  "foreground": "#1b2151"
		}
	  },
	  {
		"name": "C# namespace",
		"scope": "source.cs entity.name.type.namespace",
		"settings": {
		  "foreground": "#ffffff"
		}
	  },
	  
	],
	"colors": {
	  "focusBorder": "#32007e",
	  "foreground": "#FFFFFF",
	  "button.background": "#2e0a7dc3",
	  "button.foreground": "#ffffff",
	  "dropdown.background": "#08013B",
	  "input.background": "#130e3b",
	  "inputOption.activeBorder": "#2f00a6",
	  "list.activeSelectionBackground": "#4e2fff80",
	  "list.activeSelectionForeground": "#ffffff",
	  "list.dropBackground": "#7543ff80",
	  "list.focusBackground": "#6d2fff80",
	  "list.focusForeground": "#FFFFFF",
	  "list.highlightForeground": "#200688",
	  "list.hoverBackground": "#FFFFFF1a",
	  "list.inactiveSelectionBackground": "#FFFFFF33",
	  "activityBar.background": "#08013b",
	  "activityBarBadge.background": "#3507b3",
	  "activityBarBadge.foreground": "#ffffff",
	  "badge.background": "#3507b3",
	  "badge.foreground": "#ffffff",
	  "sideBar.background": "#08013b",
	  "sideBarSectionHeader.background": "#08013b",
	  "editorGroup.dropBackground": "#2f3dff80",
	  "editorGroup.focusedEmptyBorder": "#2f66ff",
	  "editorGroupHeader.tabsBackground": "#08013b",
	  "tab.border": "#00000000",
	  "tab.activeBorder": "#1b078f",
	  "tab.lastPinnedBorder": "#570ca2",
	  "tab.inactiveBackground": "#08013b",
	  "tab.activeBackground": "#08013b",
	  "tab.activeModifiedBorder": "#4c00ff",
	  "tab.inactiveModifiedBorder": "#441c99",
	  "tab.unfocusedActiveModifiedBorder": "#4c00ff",
	  "tab.unfocusedInactiveModifiedBorder": "#441c99",
	  "editor.background": "#08013b",
	  "editor.foreground": "#FFFFFF",
	  "editorLineNumber.foreground": "#FFFFFF4d",
	  "editorLineNumber.activeForeground": "#0d0b68",
	  "editorCursor.foreground": "#381fc3",
	  "editor.lineHighlightBorder": "#FFFFFF1a",
	  "editor.wordHighlightBackground": "#5737d8",
	  "editor.rangeHighlightBackground": "#FFFFFF0d",
	  "editorWidget.background": "#08013b",
	  "editorHoverWidget.background": "#08013b",
	  "editorMarkerNavigation.background": "#08013b",
	  "peekView.border": "#632fff",
	  "peekViewEditor.background": "#0d0715",
	  "peekViewResult.background": "#08013b",
	  "peekViewTitle.background": "#08013b",
	  "panel.background": "#08013b",
	  "panel.border": "#08013b",
	  "panelTitle.activeBorder": "#FFFFFF80",
	  "panelTitle.inactiveForeground": "#FFFFFF80",
	  "statusBar.background": "#08013b",
	  "statusBar.debuggingBackground": "#08013b",
	  "statusBar.noFolderBackground": "#08013b",
	  "statusBarItem.activeBackground": "#08013b",
	  "statusBarItem.hoverBackground": "#08013b",
	  "statusBarItem.remoteBackground": "#08013b",
	  "scrollbarSlider.background": "#3c085bad",
	  "notifications.background": "#08013b",
	  "statusBarItem.remoteForeground": "#08013b",
	  "titleBar.activeBackground": "#08013b",
	  "pickerGroup.border": "#FFFFFF1a",
	  "debugToolBar.background": "#08013b",
	  "selection.background": "#6a2fff",
	  "sash.hoverBorder": "#2f1987",
	  "editorWidget.border": "#08013b",
	  "activityBar.activeBorder": "#0d0457",
      "editorError.foreground": "#ff0000",
	}
  }